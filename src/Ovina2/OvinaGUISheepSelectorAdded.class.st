Class {
	#name : #OvinaGUISheepSelectorAdded,
	#superclass : #OvinaAnnouncement,
	#instVars : [
		'sheep'
	],
	#category : #'Ovina2-Announcements'
}

{ #category : #accessing }
OvinaGUISheepSelectorAdded >> sheep [

	^ sheep
]

{ #category : #accessing }
OvinaGUISheepSelectorAdded >> sheep: anObject [

	sheep := anObject
]
