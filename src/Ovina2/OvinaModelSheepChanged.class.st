Class {
	#name : #OvinaModelSheepChanged,
	#superclass : #OvinaAnnouncement,
	#instVars : [
		'sheep'
	],
	#category : #'Ovina2-Announcements'
}

{ #category : #accessing }
OvinaModelSheepChanged >> sheep [

	^ sheep
]

{ #category : #accessing }
OvinaModelSheepChanged >> sheep: anObject [

	sheep := anObject
]
